package net.ivarcode.camden.piece;

import java.awt.image.BufferedImage;

import net.ivarcode.camden.Location;

public class Pawn extends Piece {
	
	public Pawn(Location loc, String color, BufferedImage img) {
		super(loc, color, img);
		setID("Pawn");
		setPointValue(1);
	}
	
	public Pawn() {
		//null constructor
	}

}
